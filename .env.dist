# SpeleoDB API Test Configuration
# Copy this file to .env and fill in your actual credentials

# SpeleoDB Instance Configuration
SPELEODB_INSTANCE_URL=www.speleodb.org

# ==================== Authentication ==================== #

# Option 1: OAuth Token (recommended)
SPELEODB_OAUTH_TOKEN=your-oauth-token-here

# Option 2: Email/Password
SPELEODB_EMAIL=your-email@example.com
SPELEODB_PASSWORD=your-password-here

# ==================== Configuration ==================== #

# API Testing Configuration
API_TIMEOUT_MS=10000
API_RETRY_COUNT=3
API_TEST_ENABLED=true
